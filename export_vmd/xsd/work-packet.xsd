<?xml version="1.0"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema">
	<!-- <TreeWorkPacket> -->
	<xs:element name="TreeWorkPacket">
		<xs:complexType>
			<!-- <sAcctType> varchar(1)*			[W], R, Y, Z (see list below) -->
			<xs:simpleType name="sAcctType">
				<xs:restriction base="xs:string">
					<xs:length value="1"/>
				</xs:restriction>
			</xs:simpleType>

			<!-- <sRoleType> varchar(2)*			[PI], TT -->
			<xs:simpleType name="sRoleType">
				<xs:restriction base="xs:string">
					<xs:length value="2"/>
				</xs:restriction>
			</xs:simpleType>

			<!-- <sDT> varchar(1)*			D, [T] -->
			<xs:simpleType name="sDT">
				<xs:restriction base="xs:string">
					<xs:length value="1"/>
				</xs:restriction>
			</xs:simpleType>

			<!-- <sDivCode> varchar(2)*			User selected value (listed in appendix) -->
			<xs:simpleType name="sDivCode">
				<xs:restriction base="xs:string">
					<xs:length value="2"/>
				</xs:restriction>
			</xs:simpleType>

			<!-- <iProjID> int				[NULL] or user supplied value -->
			<xs:simpleType name="iProjID">
				<xs:restriction base="xs:integer">
					<xs:maxInclusive value="2147483647" />
					<xs:minInclusive value="-2147483648" />
				</xs:restriction>
			</xs:simpleType>

			<!-- <bQC> bit*				[0], 1 -->
			<xs:element name="">
				<xs:simpleType>
					<xs:restriction base="xs:integer">
						<xs:pattern value="[0-1]"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<!-- <bReadOnly> bit*			[0], 1 -->
			<xs:element name="">
				<xs:simpleType>
					<xs:restriction base="xs:integer">
						<xs:pattern value="[0-1]"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<!-- <sInspComp> varchar(3)			[NULL] or user supplied value from list below -->
			<xs:simpleType name="sInspComp">
				<xs:restriction base="xs:string">
					<xs:length value="3"/>
				</xs:restriction>
			</xs:simpleType>

			<!-- <sAssignedUser> varchar(50)		[NULL] or user supplied value -->
			<xs:simpleType name="sAssignedUser">
				<xs:restriction base="xs:string">
					<xs:length value="50"/>
				</xs:restriction>
			</xs:simpleType>

			<!-- <sWorkPacketStatus> char(3)		[NEW], INP, CMP -->
			<xs:simpleType name="sWorkPacketStatus">
				<xs:restriction base="xs:string">
					<xs:length value="3"/>
				</xs:restriction>
			</xs:simpleType>

			<!-- <sTransmitStatus> char(2)		[CI], CO -->
			<xs:simpleType name="sTransmitStatus">
				<xs:restriction base="xs:string">
					<xs:length value="2"/>
				</xs:restriction>
			</xs:simpleType>

			<!-- <sComments> varchar(1024)		[NULL]  or user supplied value - **Recommend using import batch ID from external system -->
			<xs:simpleType name="sComments">
				<xs:restriction base="xs:string">
					<xs:length value="1024"/>
				</xs:restriction>
			</xs:simpleType>

			<!-- <bObsolete> bit*			[0], 1 -->
			<xs:element name="">
				<xs:simpleType>
					<xs:restriction base="xs:integer">
						<xs:pattern value="[0-1]"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<!-- <bAdHoc> bit				0, [1] -->
			<xs:element name="">
				<xs:simpleType>
					<xs:restriction base="xs:integer">
						<xs:pattern value="[0-1]"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<!-- <sGenType> char(2)			[T0],TS,TR,C0,CS,CR -->
			<xs:simpleType name="sGenType">
				<xs:restriction base="xs:string">
					<xs:length value="2"/>
				</xs:restriction>
			</xs:simpleType>

			<!-- <iPacketParent>	int			[NULL] -->
			<xs:simpleType name="iPacketParent">
				<xs:restriction base="xs:integer">
					<xs:maxInclusive value="2147483647" />
					<xs:minInclusive value="-2147483648" />
				</xs:restriction>
			</xs:simpleType>

			<!-- <TreeLoc> -->
			<xs:element name="TreeLoc">
				<xs:complexType>
					<!-- <iSSDRoute> int				*Default to 10 -->
					<xs:simpleType name="iSSDRoute">
						<xs:restriction base="xs:integer">
							<xs:maxInclusive value="2147483647" />
							<xs:minInclusive value="-2147483648" />
						</xs:restriction>
					</xs:simpleType>
	
					<!-- <iRouteNum> int				*Increment locations by 10 starting at 10 -->
					<xs:simpleType name="iRouteNum">
						<xs:restriction base="xs:integer">
							<xs:maxInclusive value="2147483647" />
							<xs:minInclusive value="-2147483648" />
						</xs:restriction>
					</xs:simpleType>
	
					<!-- <sDivCode> char(2)			*Valid DIV code below -->
					<xs:simpleType name="sDivCode">
						<xs:restriction base="xs:string">
							<xs:length value="2"/>
						</xs:restriction>
					</xs:simpleType>
	
					<!-- <sCountyCode> char(2)			*Valid County Code below -->
					<xs:simpleType name="sCountyCode">
						<xs:restriction base="xs:string">
							<xs:length value="2"/>
						</xs:restriction>
					</xs:simpleType>
	
					<!-- <sInspComp> char(3) 			*[PGE] or select from list below -->
					<xs:simpleType name="sInspComp">
						<xs:restriction base="xs:string">
							<xs:length value="3"/>
						</xs:restriction>
					</xs:simpleType>
	
					<!-- <sInsp> char(4)				[SYSA] or userid of inspector -->
					<xs:simpleType name="sInsp">
						<xs:restriction base="xs:string">
							<xs:length value="4"/>
						</xs:restriction>
					</xs:simpleType>
	
					<!-- <sArea> char(10)			[NULL] -->
					<xs:simpleType name="sArea">
						<xs:restriction base="xs:string">
							<xs:length value="10"/>
						</xs:restriction>
					</xs:simpleType>
	
					<!-- <sCircuit> char(10)			*Circuit from list below -->
					<xs:simpleType name="sCircuit">
						<xs:restriction base="xs:string">
							<xs:length value="10"/>
						</xs:restriction>
					</xs:simpleType>
	
					<!-- <sQuadMap> char(11)			[NULL] -->
					<xs:simpleType name="sQuadMap">
						<xs:restriction base="xs:string">
							<xs:length value="11"/>
						</xs:restriction>
					</xs:simpleType>
	
					<!-- <sMapType> char(1)			[NULL] -->
					<xs:simpleType name="sMapType">
						<xs:restriction base="xs:string">
							<xs:length value="1"/>
						</xs:restriction>
					</xs:simpleType>
	
					<!-- <sMapTypeNum> char(8)			[NULL] -->
					<xs:simpleType name="sMapTypeNum">
						<xs:restriction base="xs:string">
							<xs:length value="8"/>
						</xs:restriction>
					</xs:simpleType>
	
					<!-- <sTagType> char(1)			*[D] or select from list below -->
					<xs:simpleType name="sTagType">
						<xs:restriction base="xs:string">
							<xs:length value="1"/>
						</xs:restriction>
					</xs:simpleType>
	
					<!-- <sTagNum> char(10)			[NULL] -->
					<xs:simpleType name="sTagNum">
						<xs:restriction base="xs:string">
							<xs:length value="10"/>
						</xs:restriction>
					</xs:simpleType>
	
					<!-- <sStreetNum> char(11)			*Use APN street number or [0] -->
					<xs:simpleType name="sStreetNum">
						<xs:restriction base="xs:string">
							<xs:length value="11"/>
						</xs:restriction>
					</xs:simpleType>
	
					<!-- <sStreet> char(26)			*Use APN street name or [NO ROAD] -->
					<xs:simpleType name="sStreet">
						<xs:restriction base="xs:string">
							<xs:length value="26"/>
						</xs:restriction>
					</xs:simpleType>
	
					<!-- <sDirections> varchar(70)				[NULL] -->
					<xs:simpleType name="sDirections">
						<xs:restriction base="xs:string">
							<xs:length value="70"/>
						</xs:restriction>
					</xs:simpleType>
	
					<!-- <sRACode1> char(2)				[NULL] or select from list below -->
					<xs:simpleType name="sRACode1">
						<xs:restriction base="xs:string">
							<xs:length value="2"/>
						</xs:restriction>
					</xs:simpleType>
	
					<!-- <sRACode2> char(2)			[NULL] or select from list below -->
					<xs:simpleType name="sRACode2">
						<xs:restriction base="xs:string">
							<xs:length value="2"/>
						</xs:restriction>
					</xs:simpleType>
	
					<!-- <sCity> char(11)			*First 11 char of city, or nearest city if in unicorporated county territory -->
					<xs:simpleType name="sCity">
						<xs:restriction base="xs:string">
							<xs:length value="11"/>
						</xs:restriction>
					</xs:simpleType>
	
					<!-- <sSourceDev> char(8)			*Use Tower ID xxx/xxx -->
					<xs:simpleType name="sSourceDev">
						<xs:restriction base="xs:string">
							<xs:length value="8"/>
						</xs:restriction>
					</xs:simpleType>
	
					<!-- <sPriVolt> char(3)			Highest Line voltage on span or [NULL] if unknown -->
					<xs:simpleType name="sPriVolt">
						<xs:restriction base="xs:string">
							<xs:length value="3"/>
						</xs:restriction>
					</xs:simpleType>
	
					<!-- <sSConst> char(1)			[NULL] -->
					<xs:simpleType name="sSConst">
						<xs:restriction base="xs:string">
							<xs:length value="1"/>
						</xs:restriction>
					</xs:simpleType>
	
					<!-- <bSRA> bit				*SRA = 1 if state/federal fire responsibility area, else 0 for LRA -->
					<xs:element name="">
						<xs:simpleType>
							<xs:restriction base="xs:integer">
								<xs:pattern value="[0-1]"/>
							</xs:restriction>
						</xs:simpleType>
					</xs:element>
					<!-- <sComments> char(255)			[NULL] -->
					<xs:simpleType name="sComments">
						<xs:restriction base="xs:string">
							<xs:length value="255"/>
						</xs:restriction>
					</xs:simpleType>
	
					<!-- <bCommAlert> bit			*[0] -->
					<xs:element name="">
						<xs:simpleType>
							<xs:restriction base="xs:integer">
								<xs:pattern value="[0-1]"/>
							</xs:restriction>
						</xs:simpleType>
					</xs:element>
					<!-- <sPCC> char(6)				[NULL] -->
					<xs:simpleType name="sPCC">
						<xs:restriction base="xs:string">
							<xs:length value="6"/>
						</xs:restriction>
					</xs:simpleType>
	
					<!-- <sMisc> char(5)				[NULL] -->
					<xs:simpleType name="sMisc">
						<xs:restriction base="xs:string">
							<xs:length value="5"/>
						</xs:restriction>
					</xs:simpleType>
	
					<!-- <sCustName> char(15)			Customer Name from APN or [NULL] -->
					<xs:simpleType name="sCustName">
						<xs:restriction base="xs:string">
							<xs:length value="15"/>
						</xs:restriction>
					</xs:simpleType>
	
					<!-- <sCustPhone> char(12)			Customer Phone from APN or [NULL] -->
					<xs:simpleType name="sCustPhone">
						<xs:restriction base="xs:string">
							<xs:length value="12"/>
						</xs:restriction>
					</xs:simpleType>
	
					<!-- <bCustAtLoc> bit			*[0] -->
					<xs:element name="">
						<xs:simpleType>
							<xs:restriction base="xs:integer">
								<xs:pattern value="[0-1]"/>
							</xs:restriction>
						</xs:simpleType>
					</xs:element>
					<!-- <sLocalID> char(2)			[NULL] -->
					<xs:simpleType name="sLocalID">
						<xs:restriction base="xs:string">
							<xs:length value="2"/>
						</xs:restriction>
					</xs:simpleType>
	
					<!-- <sPoleNum> char(9)			Tower Number or [NULL] -->
					<xs:simpleType name="sPoleNum">
						<xs:restriction base="xs:string">
							<xs:length value="9"/>
						</xs:restriction>
					</xs:simpleType>
	
					<!-- <sLineID> char(6)			Highest voltage Transmission Line on span -->
					<xs:simpleType name="sLineID">
						<xs:restriction base="xs:string">
							<xs:length value="6"/>
						</xs:restriction>
					</xs:simpleType>
	
					<!-- <dtInspDate> datetime			Inspection Date (flyover date) -->
					<xs:element name="dtInspDate" type="xs:date"/>
	
					<!-- <sRemNum> char(8)			[NULL] -->
					<xs:simpleType name="sRemNum">
						<xs:restriction base="xs:string">
							<xs:length value="8"/>
						</xs:restriction>
					</xs:simpleType>
	
					<!-- <sAcctType> char(1)			[W], R, Y, Z or from list below -->
					<xs:simpleType name="sAcctType">
						<xs:restriction base="xs:string">
							<xs:length value="1"/>
						</xs:restriction>
					</xs:simpleType>
	
					<!-- <sCustName2> char(15)			[NULL] or user entered -->
					<xs:simpleType name="sCustName2">
						<xs:restriction base="xs:string">
							<xs:length value="15"/>
						</xs:restriction>
					</xs:simpleType>
	
					<!-- <sCustPhone2> char(12)			[NULL] or user entered -->
					<xs:simpleType name="sCustPhone2">
						<xs:restriction base="xs:string">
							<xs:length value="12"/>
						</xs:restriction>
					</xs:simpleType>
	
					<!-- <sRACode3> char(2)			[NULL] or select from list below -->
					<xs:simpleType name="sRACode3">
						<xs:restriction base="xs:string">
							<xs:length value="2"/>
						</xs:restriction>
					</xs:simpleType>
	
					<!-- <sRACode4> char(2)			[NULL] or select from list below -->
					<xs:simpleType name="sRACode4">
						<xs:restriction base="xs:string">
							<xs:length value="2"/>
						</xs:restriction>
					</xs:simpleType>
	
					<!-- <sRACode5> char(2)			[NULL] or select from list below -->
					<xs:simpleType name="sRACode5">
						<xs:restriction base="xs:string">
							<xs:length value="2"/>
						</xs:restriction>
					</xs:simpleType>
	
					<!-- <sRACode6> char(2)			[NULL] or select from list below -->
					<xs:simpleType name="sRACode6">
						<xs:restriction base="xs:string">
							<xs:length value="2"/>
						</xs:restriction>
					</xs:simpleType>
	
					<!-- <bUnderBuild> bit			[0],1 - Not sure if it's possible to visually determine if there is distrubution under the trans line -->
					<xs:element name="">
						<xs:simpleType>
							<xs:restriction base="xs:integer">
								<xs:pattern value="[0-1]"/>
							</xs:restriction>
						</xs:simpleType>
					</xs:element>
					<!-- <iPIRoute> int				Incrementing number in order of inspection (1,2,3,... etc) -->
					<xs:simpleType name="iPIRoute">
						<xs:restriction base="xs:integer">
							<xs:maxInclusive value="2147483647" />
							<xs:minInclusive value="-2147483648" />
						</xs:restriction>
					</xs:simpleType>
	
					<!-- <sXStreet> char(255)			[NULL] or Nearest cross street or NULL if none within 1 mile -->
					<xs:simpleType name="sXStreet">
						<xs:restriction base="xs:string">
							<xs:length value="255"/>
						</xs:restriction>
					</xs:simpleType>
	
					<!-- <sAPN> char(50)				[NULL] or APN number -->
					<xs:simpleType name="sAPN">
						<xs:restriction base="xs:string">
							<xs:length value="50"/>
						</xs:restriction>
					</xs:simpleType>
	
					<!-- <sPoleNum2> char(9)			[NULL] or Next Tower Number in span (xxx/xxx) -->
					<xs:simpleType name="sPoleNum2">
						<xs:restriction base="xs:string">
							<xs:length value="9"/>
						</xs:restriction>
					</xs:simpleType>
	
					<!-- <iEasement> int				[NULL] -->
					<xs:simpleType name="iEasement">
						<xs:restriction base="xs:integer">
							<xs:maxInclusive value="2147483647" />
							<xs:minInclusive value="-2147483648" />
						</xs:restriction>
					</xs:simpleType>
	
					<!-- <iSpanLength> int			[NULL] or Distance in ft. between tower 1 and tower 2 -->
					<xs:simpleType name="iSpanLength">
						<xs:restriction base="xs:integer">
							<xs:maxInclusive value="2147483647" />
							<xs:minInclusive value="-2147483648" />
						</xs:restriction>
					</xs:simpleType>
	
					<!-- <iBioUnitsTrim> int			[NULL] -->
					<xs:simpleType name="iBioUnitsTrim">
						<xs:restriction base="xs:integer">
							<xs:maxInclusive value="2147483647" />
							<xs:minInclusive value="-2147483648" />
						</xs:restriction>
					</xs:simpleType>
	
					<!-- <iBioUnitsRemoval> int			[NULL] -->
					<xs:simpleType name="iBioUnitsRemoval">
						<xs:restriction base="xs:integer">
							<xs:maxInclusive value="2147483647" />
							<xs:minInclusive value="-2147483648" />
						</xs:restriction>
					</xs:simpleType>
	
					<!-- <bVELBArea> bit				Location within Valley Elderberry protection zone = 1, else 0 -->
					<xs:element name="">
						<xs:simpleType>
							<xs:restriction base="xs:integer">
								<xs:pattern value="[0-1]"/>
							</xs:restriction>
						</xs:simpleType>
					</xs:element>
					<!-- <sTWADocNum> char(15)			[NULL] -->
					<xs:simpleType name="sTWADocNum">
						<xs:restriction base="xs:string">
							<xs:length value="15"/>
						</xs:restriction>
					</xs:simpleType>
	
					<!-- <sTWAAgreement> char(4000)		[NULL] -->
					<xs:simpleType name="sTWAAgreement">
						<xs:restriction base="xs:string">
							<xs:length value="4000"/>
						</xs:restriction>
					</xs:simpleType>
	
					<!-- <sTWASignedBy> char(50)			[NULL] -->
					<xs:simpleType name="sTWASignedBy">
						<xs:restriction base="xs:string">
							<xs:length value="50"/>
						</xs:restriction>
					</xs:simpleType>
	
					<!-- <gTWASignature> BLOB			[NULL] -->
					<xs:element name="blobsrc" type="xs:hexBinary"/>
					<!-- <dtTWASignedDate> datetime		[NULL] -->
					<xs:element name="dtTWASignedDate" type="xs:date"/>
	
					<!-- <iCityID> int				From city list below -->
					<xs:simpleType name="iCityID">
						<xs:restriction base="xs:integer">
							<xs:maxInclusive value="2147483647" />
							<xs:minInclusive value="-2147483648" />
						</xs:restriction>
					</xs:simpleType>
	
					<!-- <sTreeLocStatus> char(10)		[CMP_WK_NR], or value from list below -->
					<xs:simpleType name="sTreeLocStatus">
						<xs:restriction base="xs:string">
							<xs:length value="10"/>
						</xs:restriction>
					</xs:simpleType>
	
					<!-- <bHCPArea> bit				[NULL] -->
					<xs:element name="">
						<xs:simpleType>
							<xs:restriction base="xs:integer">
								<xs:pattern value="[0-1]"/>
							</xs:restriction>
						</xs:simpleType>
					</xs:element>
					<!-- <sHCPAreaName> char(50)			[NULL] -->
					<xs:simpleType name="sHCPAreaName">
						<xs:restriction base="xs:string">
							<xs:length value="50"/>
						</xs:restriction>
					</xs:simpleType>
	
					<!-- <bE10cActivity> bit			[NULL] -->
					<xs:element name="">
						<xs:simpleType>
							<xs:restriction base="xs:integer">
								<xs:pattern value="[0-1]"/>
							</xs:restriction>
						</xs:simpleType>
					</xs:element>
					<!-- <iE10cAcreage> int			[NULL] -->
					<xs:simpleType name="iE10cAcreage">
						<xs:restriction base="xs:integer">
							<xs:maxInclusive value="2147483647" />
							<xs:minInclusive value="-2147483648" />
						</xs:restriction>
					</xs:simpleType>
	
					<!-- <bMBZ> bit				[NULL] -->
					<xs:element name="">
						<xs:simpleType>
							<xs:restriction base="xs:integer">
								<xs:pattern value="[0-1]"/>
							</xs:restriction>
						</xs:simpleType>
					</xs:element>
					<!-- <sMBZID> char(8)			[NULL] -->
					<xs:simpleType name="sMBZID">
						<xs:restriction base="xs:string">
							<xs:length value="8"/>
						</xs:restriction>
					</xs:simpleType>
	
					<!-- <sJointPoleUtility> char(60)		[NULL] -->
					<xs:simpleType name="sJointPoleUtility">
						<xs:restriction base="xs:string">
							<xs:length value="60"/>
						</xs:restriction>
					</xs:simpleType>
	
					<!-- <dtPrevInspDate> datetime		[NULL] -->
					<xs:element name="dtPrevInspDate" type="xs:date"/>
	
					<!-- <ExternalLocID> varchar(50)		Unique identifier of location from external source system -->
					<xs:simpleType name="ExternalLocID">
						<xs:restriction base="xs:string">
							<xs:length value="50"/>
						</xs:restriction>
					</xs:simpleType>
	
					<!-- <TreeLoc_GPS> -->
					<xs:element name="TreeLoc_GPS">
						<xs:complexType>
							<!-- <nLatitude> numeric(12, 9)		format:   39.736025000 -->
							<xs:simpleType name="nLatitude">
								<xs:restriction base="xs:decimal">
									<xs:fractionDigits value="9" />
									<xs:totalDigits value="12" />                    
								</xs:restriction>
							</xs:simpleType>
							<!-- <nLongitude> numeric(12, 9)		format: -121.859128000 -->
							<xs:simpleType name="nLongitude">
								<xs:restriction base="xs:decimal">
									<xs:fractionDigits value="9" />
									<xs:totalDigits value="12" />                    
								</xs:restriction>
							</xs:simpleType>
							<!-- <dtMobileCapture> datetime		Date/Time of flyover -->
							<xs:element name="dtMobileCapture" type="xs:date"/>
		
			
						</xs:complexType>        
					</xs:element>
					<!-- <TreeLocAlerts> -->
					<xs:element name="TreeLocAlerts">
						<xs:complexType>
							<!-- <iTreeLocID> int			*LocationID -->
							<xs:simpleType name="iTreeLocID">
								<xs:restriction base="xs:integer">
									<xs:maxInclusive value="2147483647" />
									<xs:minInclusive value="-2147483648" />
								</xs:restriction>
							</xs:simpleType>
		
