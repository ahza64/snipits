									<xs:element name="">
										<xs:simpleType>
											<xs:restriction base="xs:integer">
												<xs:pattern value="[0-1]"/>
											</xs:restriction>
										</xs:simpleType>
									</xs:element>
									<!-- <sUserID> varchar(50) 			*[UserID] -->
									<xs:simpleType name="sUserID">
										<xs:restriction base="xs:string">
											<xs:length value="50"/>
										</xs:restriction>
									</xs:simpleType>
			
									<!-- <dtDateAdded> datetime 			*[Inspection Date] -->
									<xs:element name="dtDateAdded" type="xs:date"/>
			
				
								</xs:complexType>        
							</xs:element>
							<!-- <TreeRecsFile>					Note that <TreeRecsFile> section can be repeated for multiple files -->
							<xs:element name="TreeRecsFile">
								<xs:complexType>
									<!-- <iTreeRecsID> int			*File will be associated with this tree -->
									<xs:simpleType name="iTreeRecsID">
										<xs:restriction base="xs:integer">
											<xs:maxInclusive value="2147483647" />
											<xs:minInclusive value="-2147483648" />
										</xs:restriction>
									</xs:simpleType>
			
									<!-- <sFileName> varchar(100)		*Should conform to windows file naming standards - no special chars such as ? , etc... -->
									<xs:simpleType name="sFileName">
										<xs:restriction base="xs:string">
											<xs:length value="100"/>
										</xs:restriction>
									</xs:simpleType>
			
									<!-- <sFile> BLOB data stream		*No data limit -->
									<xs:element name="blobsrc" type="xs:hexBinary"/>
				
								</xs:complexType>        
							</xs:element>
			
						</xs:complexType>        
					</xs:element>
		
				</xs:complexType>        
			</xs:element>
	
		</xs:complexType>        
	</xs:element>

</xs:schema>

